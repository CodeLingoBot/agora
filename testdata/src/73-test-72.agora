rnge := import("72-range-impl")
fmt := import("fmt")

func forLoop(fn) {
	// TODO : Status return empty string instead of "func"? So that it can be used as bool?
	for v := fn(); status(fn) == "suspended"; v = fn() {
		fmt.Println(v)
	}
	if status(fn) != "func" {
		panic("expected status to be back to func")
	}
}

fmt.Println("range(6)")
forLoop(rnge(6))
fmt.Println("range(0)")
forLoop(rnge(0))
fmt.Println("range(2, 7)")
forLoop(rnge(2, 7))
fmt.Println("range(2, 7, 3)")
forLoop(rnge(2, 7, 3))
fmt.Println("range(2, -4)")
forLoop(rnge(2, -4))
fmt.Println("range(2, -4, -1)")
forLoop(rnge(2, -4, -1))
fmt.Println("range(-2, 4)")
forLoop(rnge(-2, 4))
fmt.Println("range(-2, 4, -1)")
forLoop(rnge(-2, 4, -1))
