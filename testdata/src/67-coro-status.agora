/*---
result: 4
output: func\nrunning\nsuspended\nfunc\nnumber\n
---*/

fmt := import("fmt")

func fn(n) {
	fmt.Println(status(fn))
	i := yield
	i = yield
	return (n + i)
}

fmt.Println(status(fn))
fn(1)
fmt.Println(status(fn))
fn(2)
a := fn(3)
fmt.Println(status(fn))
fmt.Println(status(a))
return a
